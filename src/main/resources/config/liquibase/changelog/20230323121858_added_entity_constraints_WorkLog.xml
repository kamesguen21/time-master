<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity WorkLog.
    -->
    <changeSet id="20230323121858-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="work_log"
                                 constraintName="fk_work_log__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_config"/>

        <addForeignKeyConstraint baseColumnNames="ticket_id"
                                 baseTableName="work_log"
                                 constraintName="fk_work_log__ticket_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ticket"/>
    </changeSet>
</databaseChangeLog>
